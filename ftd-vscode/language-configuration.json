{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    [
      "{",
      "}"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ]
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ]
  ],
  "folding": {
    "markers": {
      "start": "\\{",
      "end": "\\}"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "\\{\\s*$",
    "decreaseIndentPattern": "^\\s*\\}"
  }
}